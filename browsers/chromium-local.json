{
  "environment" : "local",
  "capabilities" : {
    "browserName" : "chromium",
    "goog:chromeOptions" : {
      "binary" : "%FILE:CHROMIUM%",
      "args" : [
          "--use-gl=swiftshader-webgl",
	  "--no-sandbox"
      ]
    },
    "google:wslConfig": {
      "binary": "%FILE:CHROMEDRIVER%",
      "port":"%WSLPORT:WSL%",
      "args": ["--port=%WSLPORT:WSL%"]
    }
  }
}
